{
  "name": "bertoost/craft-mailjet",
  "description": "Craft CMS plugin for adding Mailjet mail adapter.",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "craftcms",
    "plugin",
    "craftcms plugin",
    "mailjet",
    "mailer",
    "adapter"
  ],
  "homepage": "https://github.com/bertoost/Craft-3-Mailjet",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "craftcms/cms": "^4.1",
    "mailjet/mailjet-apiv3-php": "^1.5.6",
    "symfony/mailjet-mailer": "^v6.1"
  },
  "autoload": {
    "psr-4": {
      "bertoost\\mailjet\\": "src/"
    }
  },
  "extra": {
    "name": "Mailjet",
    "handle": "mailjet",
    "hasCpSettings": true,
    "developer": "Bert Oost",
    "developerUrl": "https://bertoost.com",
    "documentationUrl": "https://github.com/bertoost/Craft-3-Mailjet/blob/master/README.md"
  }
}
